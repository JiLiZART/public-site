{
  "name": "public-site",
  "version": "1.0.0",
  "description": "Vue 2 version of artkost.ru build on top of nuxt.js",
  "author": "Nikolay Kostyurin <jilizart@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3333"
    }
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.3",
    "cross-env": "^5.1.3",
    "date-fns": "^1.29.0",
    "front-matter": "^2.3.0",
    "koa": "^2.4.1",
    "nuxt": "^1.0.0-rc11",
    "slugify": "^1.2.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
